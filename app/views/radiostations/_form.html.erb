<%= simple_form_for(@radiostation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :streaming_url %>
    <%= f.input :name %>
    <%= f.input :logo, as: :file %>
    <%= f.input :description %>
    <%= f.input :active %>
    <%= f.input :online %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
